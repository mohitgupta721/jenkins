#!/usr/bin/env groovy

pipeline {
   agent any
   environment {
       color = "blue"
   }
   stages {
       stage('Get source from bitbucket') {
            agent { label 'master' }
            steps {
               git credentialsId: 'mohit-gupta', url: 'https://mohit-gupta@bitbucket.org/dhwanidevs/jagrutkrishak_mobile.git'
               
            }
       }}
}
    
